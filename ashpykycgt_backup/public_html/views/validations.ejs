<%if(typeof errors !='undefined'){%>
<%errors.forEach(function (error) {  %>
<div class="toast  fade show alert p-0 " role="alert" data-autohide="false" aria-live="assertive" aria-atomic="true">
    <div class="toast-header">
        <i class="fa fa-2x fa-exclamation-circle mr-2 text-danger"></i>
        <strong class="mr-auto">CompleteGreet</strong>
        <button type="button" class="ml-2 mb-1 close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div class="toast-body">
        <%=error.msg%>
    </div>
</div>



<%});%>
<%}%>
<%if (error_msg!=''){  %>
<div class="toast Register-Err fade show alert p-0 " role="alert" data-autohide="false" aria-live="assertive" aria-atomic="true">
    <div class="toast-header">
        <i class="fa fa-2x fa-exclamation-circle mr-2 text-danger"></i>
        <strong class="mr-auto">CompleteGreet</strong>
        <button type="button" class="ml-2 mb-1 close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div class="toast-body">
        <%=error_msg%>
    </div>
</div>

<%};%>
<%if (Success_msg!=''){  %>
<div class="toast Toast-Success mb-3" style="opacity: 1" data-delay="3000" role="alert" aria-live="assertive" aria-atomic="true">
    <div class="toast-header">
        <i class="fa fa-2x fa-check-circle mr-2 text-success"></i>
        <strong class="mr-auto ">Complete Greet</strong>
        <small class="text-white">just now</small>
        <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div class="toast-body">
        <%=Success_msg%>
    </div>
</div>

<%};%>
<%if (errorn!=''){  %>
<div class="alert alert-danger alert-dismissible fade show" role="alert">
    <%=errorn%>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>

<%};%>
<div class="toast Register-Err fade hide alert p-0 " role="alert" data-autohide="false" aria-live="assertive" aria-atomic="true">
    <div class="toast-header">
        <i class="fa fa-2x fa-exclamation-circle mr-2 text-danger"></i>
        <strong class="mr-auto">CompleteGreet</strong>
        <button type="button" class="ml-2 mb-1 close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div class="toast-body">

    </div>
</div>
