<!-- Alerts-->
<%if (Success_msg!=''){  %>
    <div class="toast inbodyUploadToast bg-success p-1 border-0" data-autohide="true" data-autohide="false"  style="width:350px;opacity: 1;position: fixed; z-index: 10000; bottom: 2%; right: 2%;">
        <div class="toast-header bg-success text-white border-0">

            <strong class="mr-auto">Inbody Uploaded</strong>
            <small class="text-white">Now</small>
            <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    </div>
<%}%>
<%if (error_msg!=''){  %>
<div class="toast inbodyUploadToast bg-danger p-1 border-0" data-autohide="true" data-autohide="false"  style="width:350px;opacity: 1;position: fixed; z-index: 10000; bottom: 2%; right: 2%;">
    <div class="toast-header bg-danger text-white border-0">

        <strong class="mr-auto"><%=error_msg%></strong>
        <small class="text-white">Now</small>
        <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
</div>
