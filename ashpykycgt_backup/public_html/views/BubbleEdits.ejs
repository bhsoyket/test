<% include toasts.ejs%>

<div class="modal fade" id="Record-Modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Record Video</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12">
                        <h2>Preview</h2>
                        <video id="preview"  autoplay muted></video>
                    </div>
                    <div class="col-12 d-flex justify-content-around my-4">
                        <div id="startButton" style="width: fit-content" class="Button ">Start Recording</div>
                        <div id="stopButton" style="width: fit-content" class="Button ">Stop Recording</div>

                    </div>

                </div>
                <div class="right">
                    <video id="recording" class="d-none"  controls></video>
                    <!--                        <a id="downloadButton" class="Button"> Download </a>-->
                </div>
                <div class="left">

                </div>
                <div class="bottom">
                    <pre id="log"></pre>
                </div>
            </div>
            <div class="modal-footer">
                <a type="button" id="Delete-Record" class="Button" data-dismiss="modal">Close</a>
                <a type="button" id="Save-Record" class="Button">Save</a>
            </div>
        </div>
    </div>
</div>
<div class="progress-parent  d-none">
    <div class="progress upload-progress-bar d-flex position-fixed justify-content-between align-items-center  ">

        <div class="progress-bar mr-2 h-75" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">0%</div>
        <div class="upload-icon" style="color: #3B5DCD"><i class="fa-solid fa-cloud-arrow-up fa-xl"></i></div>
    </div>
</div>

<script>
    var Bubble=<%-JSON.stringify(Bubble)%>
</script>
<script>
     var Chats=<%-JSON.stringify(Chats)%>
     var UserID='<%=user.id%>'
         var UserPushNotification='<%=user.PushNotification%>'
     var NotificationSound="<%=System[0].NotificationSound%>"
