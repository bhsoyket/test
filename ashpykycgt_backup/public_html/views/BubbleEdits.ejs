<body class="hold-transition sidebar-mini">



<%  include UserNavBar.ejs%>
<div  class="page-container">
    <div class="container-row d-flex flex-row ">
        <%  include UsersSideBar.ejs%>
        <div class="page-content-column mt-5 w-100 mr-lg-0">
            <div class="tab-content" id="pills-tabContent">
                <div class="tab-pane fade  show active" id="Bubble-Creation" role="tabpanel" aria-labelledby="Bubble-Creation-tab">
                    <div class="row mr-2">
                        <div class="col-12">
                            <div class="Bubble-Creation-Nav d-flex flex-row flex-md-row">
                                <div class="Bubble-Creation-Title">
                                    Edit Bubble
                                </div>
                                <div class="Bubble-Creation-Actions d-flex ml-auto">
                                    <a id="" class="Button Bubble-Save ml-2">Save</a>
<!--                                    <a id="Bubble-Share" class="Button ml-2">Share</a>-->
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-5 col-12 mt-5 mb-4">
                            <div class="Bubble-Creation-Style-Container px-3 pt-4 d-flex flex-column align-items-center">
                                <img class="BubbleCreation-img mb-2" src="/images/CompleteGreet/FavIcon.png">
                                <label class="Input-Style w-100 mb-2 mx-2">
                                    <input name="LiveChat-Name" maxlength="20" value="<%=Bubble.BubbleName%>" id="LiveChat-Name" type="text" class="w-100" placeholder=" ">
                                    <span>Name on live chat*</span>
                                </label>
                                <div class="d-flex align-items-center flex-row mb-3">
                                    <a id="Record-Button" type="button" class="Button ml-2 text-nowrap" data-toggle="modal" data-target="#Record-Modal" class="Button ml-2">New Record</a>

                                    <span class="file-wrapper ml-2 d-flex justify-content-center">
                                    <form id="Video-Upload2" enctype="multipart/form-data" method="post">

                                     <input type="file" accept="video/*" name="Bubble-Video-file" id="Video-Upload-input2" />
                                     <span class="Button text-nowrap">Change video</span>
                                    </form>
                                </span>
                                </div>

                                <div class="Bubble-Style-Title d-flex flex-row  align-items-center w-100">
                                    <span class="mr-2">Title:</span>
                                    <input maxlength="25" type="text" id="Bubble-Style-Title-Input" value="Hey!" class="Bubble-Style-Title-Input w-100 p-2">
                                </div>
                                <div class="Bubble-Font-Size-Range mb-3 w-100">
                                    <span class="">Title font size:</span>
                                    <div class="d-flex flex-row align-items-center">
                                        <input id="font-size-range" type="range" class="input-range--custom mr-2" value="<%=Bubble.BubbleFontSize%>" min="15" max="30">
                                        <input type="number" id="font-size-Input" min="15" value="<%=Bubble.BubbleFontSize%>" max="30" class="Bubble-Style-Title-Input w-25 p-2">
                                    </div>
                                </div>
                                <div class="Bubble-Size w-100 mb-2">
                                    <span class="">Size:</span>
                                    <div class="d-flex flex-row align-items-center mb-3">
                                        <input id="Bubble-Size-range" type="range" class="input-range--custom mr-2" value="<%=Bubble.BubbleSize%>" min="50" max="300">
                                        <input type="number" id="Bubble-Size-Input" min="50" value="<%=Bubble.BubbleSize%>" max="300" class="Bubble-Style-Title-Input w-25 p-2">
                                    </div>
                                </div>
                                <div class="Bubble-Border-Color mb-3 w-100">
                                    <span class="">Border Color (HEX):</span>
                                    <div class="d-flex flex-row align-items-center mt-2 ">
                                        <input id="Bubble-Border-Color-Picker" type="color" value="<%=Bubble.BubbleBorderColor%>" class="Input-color mr-2" >
                                        <input type="text" id="Bubble-Border-Color-Input" value="<%=Bubble.BubbleBorderColor%>"  class="Bubble-Style-Title-Input w-50 p-2">
                                    </div>
                                </div>
                                <div class="Bubble-BackGround-Color mb-3 w-100">
                                    <span class="">BackGround Color (HEX):</span>
                                    <div class="d-flex flex-row align-items-center mt-2 ">
                                        <input id="Bubble-BackGround-Color-Picker" type="color" value="<%=Bubble.BubbleBackgroundColor%>" class="Input-color mr-2" >
                                        <input type="text" id="Bubble-BackGround-Color-Input" value="<%=Bubble.BubbleBackgroundColor%>"  class="Bubble-Style-Title-Input w-50 p-2">
                                    </div>
                                </div>
                                <div class="Bubble-Button-Color mb-3 w-100">
                                    <span class="">Button Color (HEX):</span>
                                    <div class="d-flex flex-row align-items-center mt-2 ">
                                        <input id="Bubble-Button-Color-Picker" type="color" value="<%=Bubble.BubbleButtonColor%>" class="Input-color mr-2" >
                                        <input type="text" id="Bubble-Button-Color-Input" value="<%=Bubble.BubbleButtonColor%>"  class="Bubble-Style-Title-Input w-50 p-2">
                                    </div>
                                </div>
                                <div class="Bubble-Font-Family mb-3 w-100 d-flex flex-column">
                                    <span class="mb-2">Font Family:</span>
                                    <select class="Font-Family-Select " value="<%=Bubble.BubbleFontFamily%>" id="Bubble-Title-Font-Family-Select">
                                        <option value="Helvetica" >Helvetica</option>
                                        <option value="Arial" >Arial</option>
                                        <option value="Arial Black">Arial Black</option>
                                        <option value="Verdana">Verdana</option>
                                        <option value="Tahoma">Tahoma</option>
                                        <option value="Impact">Impact</option>
                                        <option value="Gill Sans">Gill Sans</option>
                                        <option value="Times New Roman">Times New Roman</option>
                                        <option value="Georgia">Georgia</option>
                                        <option value="Palatino">Palatino</option>
                                        <option value="Baskerville">Baskerville</option>
                                        <option value="Andalé Mono">Andalé Mono</option>
                                        <option value="Courier">Courier</option>
                                        <option value="Lucida">Lucida</option>
                                        <option value="Monaco">Monaco</option>
                                        <option value="Bradley Hand">Bradley Hand</option>
                                        <option value="Brush Script MT">Brush Script MT</option>
                                        <option value="Luminari">Luminari</option>
                                        <option value="Comic Sans MS">Comic Sans MS</option>
                                        <option value="Trebuchet MS">Trebuchet MS</option>
                                    </select>
                                </div>
                                <div class="Bubble-Darken mb-3 w-100 d-flex flex-column">
                                    <span class="mb-2">Darken for text readability:</span>
                                    <div class="center">
                                        <input <%if(Bubble.BubbleDarken){%><%='checked'%><%}%> type="checkbox" id="Bubble-Darken" >
                                    </div>
                                </div>
                                <div class="Bubble-Style mb-3 w-100">
                                    <span class="">Style:</span>
                                    <div class="d-flex flex-column align-items-center mt-2 ">

                                        <input checked type="radio" <%if(Bubble.BubbleStyle=="Circle"){%><%='checked'%><%}%> value="Circle" id="Style-Circle" name="Style">
                                        <label class="Bubble-style-label py-2 pr-2 w-100" for="Style-Circle">
                                            <span>Round</span>
                                            <div class="style-container">
                                                <div class="circle"></div>
                                            </div>
                                        </label>


                                        <input type="radio" <%if(Bubble.BubbleStyle=="Rectangle"){%><%='checked'%><%}%> value="Rectangle" id="Style-Rectangle" name="Style">
                                        <label class="Bubble-style-label py-2 pr-2 w-100" for="Style-Rectangle">
                                            <span>Rectangular</span>
                                            <div class="style-container">
                                                <div class="rectangle"></div>
                                            </div>
                                        </label>

                                    </div>
                                </div>
                                <div class="Bubble-Position mb-3 w-100">
                                    <span class="">Position:</span>
                                    <div class="d-flex flex-column align-items-center mt-2 ">

                                        <input checked type="radio" <%if(Bubble.BubblePosition=="Right"){%><%='checked'%><%}%> value="Right" id="Style-Right" name="Position">
                                        <label class="Bubble-style-label py-2 pr-2 w-100" for="Style-Right">
                                            <span>Right Bottom</span>
                                            <div class="style-container">
                                                <div class="circle"></div>
                                            </div>
                                        </label>


                                        <input type="radio" <%if(Bubble.BubblePosition=="Left"){%><%='checked'%><%}%> id="Style-Left" value="Left" name="Position">
                                        <label class="Bubble-style-label py-2 pr-2 w-100" for="Style-Left">
                                            <span>Left Bottom</span>
                                            <div class="style-container">
                                                <div class="circle position-left"></div>
                                            </div>
                                        </label>

                                    </div>
                                </div>
                                <div class="Bubble-Video-Fit mb-3 w-100 d-flex flex-column">
                                    <span class="mb-2">Video fit:</span>
                                    <div class="center">
                                        <input <%if(Bubble.BubbleVideoFit){%><%='checked'%><%}%> type="checkbox" id="Bubble-Video-Fit" >
                                    </div>
                                </div>
                                <div class="Bubble-Delay w-100 mb-2">
                                    <span class="">Delay:</span>
                                    <div class="d-flex flex-row align-items-center mb-3">
                                        <input id="Bubble-Delay-range" type="range" value="<%=Bubble.BubbleDelay%>" class="input-range--custom mr-2" min="0" max="20">
                                        <input type="number" id="Bubble-Delay-Input" min="0" value="<%=Bubble.BubbleDelay%>" max="20" class="Bubble-Style-Title-Input w-25 p-2">
                                    </div>
                                </div>
                                <div class="Bubble-Enter-Animation mb-3 w-100 d-flex flex-column">
                                    <span class="mb-2">Animation:</span>
                                    <select class="Enter-Animation-Select " id="Bubble-Enter-Animation-Select">
                                        <option value="No-Animation" selected>No Animation</option>
                                        <option value="Right-to-left">Right to left</option>
                                        <option value="Left-to-right">Left to right</option>
                                        <option value="Top-to-bottom">Top to bottom</option>
                                    </select>
                                </div>
                                <div class="Bubble-Style-Title mb-3 d-flex flex-column  align-items-start w-100">
                                    <span class="mr-2 mb-2">First Message:</span>
                                    <textarea style="min-height: 40px" id="GreetMsg-Input" placeholder="bubble video message " class="Msg-Input Bubble-Style-Title-Input w-100" type="text"><%=Bubble.BubbleGreetMsg%></textarea>
                                    <div class="mb-2" style="color:#3B5DCD ;font-size: 12px">leave empty to ignore first message</div>
                                    <span class="">First Message Delay(s):</span>
                                    <div class="d-flex flex-row align-items-center w-100 ">
                                        <input id="Bubble-FirstMesssage-Delay-range" type="range" value="<%=Bubble.BubbleFirstMessageDelay%>" class="input-range--custom mr-2" min="0" max="20">
                                        <input type="number" id="Bubble-FirstMesssage-Delay-Input" min="0" value="<%=Bubble.BubbleFirstMessageDelay%>" max="20" class="Bubble-Style-Title-Input w-25 p-2">
                                    </div>
                                </div>
                                <div class="Bubble-Pages mb-3 w-100">
                                    <span class="">Show on:</span>
                                    <div class="d-flex url-container flex-column align-items-center mt-2 ">

                                        <input <%if(Bubble.BubbleAllPages){%><%='checked'%><%}%> type="radio" value="All-Pages" id="Style-All" name="Pages">
                                        <label class="Bubble-Pages-label py-1 pr-2 w-100" for="Style-All">
                                            <span>All pages</span>

                                        </label>


                                        <input  <%if(!Bubble.BubbleAllPages){%><%='checked'%><%}%> type="radio" value="All-Pages-Except" id="Style-AllExc" name="Pages">
                                        <label class="Bubble-Pages-label py-1 pr-2 w-100" for="Style-AllExc">
                                            <span>All page except</span>

                                        </label>
                                        <%; if (Bubble.BubbleExcPages!=null&&Bubble.BubbleExcPages!=""){var ExcludedPages = Bubble.BubbleExcPages; ExcludedPages = ExcludedPages.replace('[[', '[').replace(']]', ']').replace(/'/g, '"') ; ExcludedPages = JSON.parse(ExcludedPages)%>
                                        <%ExcludedPages.forEach(function (page,index){%>
                                        <div class="row mt-2 justify-content-center align-items-center w-100">
                                            <div class="col-10 pl-0">
                                                <input placeholder="e.g. /cart" type="text" value="<%=page%>" class="Bubble-Style-Title-Input Bubble-ExcPages-Input w-100 p-2">

                                            </div>
                                            <%if(index==0){%>
                                                <div class="col-2 mb-2 p-0">
                                                    <div class="add-url url-actions text-success">
                                                        <i class="fa-solid fa-plus"></i>
                                                    </div>
                                                </div>
                                            <%}else {%>
                                                <div class="col-2 mb-2 p-0">
                                                    <div class="remove-url url-actions text-danger">
                                                        <i class="fa-solid fa-trash"></i>
                                                    </div>
                                                </div>

                                           <%}%>
                                        </div>
                                        <%})}else {%>
                                        <div class="row mt-2 justify-content-center align-items-center w-100">
                                            <div class="col-10 pl-0">
                                                <input placeholder="e.g. /cart" type="text" value="" class="Bubble-Style-Title-Input Bubble-ExcPages-Input w-100 p-2">

                                            </div>
                                                <div class="col-2 mb-2 p-0">
                                                    <div class="add-url url-actions text-success">
                                                        <i class="fa-solid fa-plus"></i>
                                                    </div>
                                                </div>

                                        </div>
                                        <%}%>

                                    </div>
                                </div>
                                <div class="Bubble-Video-Fit mb-3 w-100 d-flex flex-column">
                                    <span class="mb-2">Deactivate bubble:</span>
                                    <div class="center">
                                        <input <%if(Bubble.BubbleDeactivated){%><%='checked'%><%}%> type="checkbox" id="Bubble-Deactivate" >
                                    </div>
                                </div>

                                <div class="Bubble-Creation-Actions d-flex mb-4 w-100 ">
<!--                                    <a id="Bubble-Reset" class="Button w-100 ml-2">Reset</a>-->
                                    <a id="" class="Button w-100 ml-2 Bubble-Save">Save</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-7 mb-lg-4 pr-lg-0 browser-box  col-12 mt-5 mb-4 d-flex flex-column align-items-center">
                            <div class="browser ">
                                <div class="browser-navigation-bar">
                                    <i></i><i></i><i></i>
                                    <!-- Place your URL into <input> below -->
                                    <input class="browser-tab-input" value="https://yourwebsite.com" disabled />
                                </div>

                                <div class="browser-container position-relative">
                                    <!-- Place your content of any type here -->
                                    <div id="Bubble-Body" class="Bubble-Body">
                                        <div class="Bubble-Inner-Container w-100 h-100 position-relative">
                                            <div class="Bubble-Video w-100 h-100">
                                                <video class="video-content  w-100 h-100" autoplay loop id="video-bubble" muted src="/files/users/<%=Bubble.UserId%>/Bubble-Videos/<%=Bubble.BubbleVideo%>">"></video>

                                            </div>
                                            <div class="Bubble-Video-Overlay">
                                                <div class="Bubble-Inner-Title text-white p-2">
                                                    Hey!
                                                </div>
                                            </div>
                                            <div class="Bubble-external-overlay">

                                            </div>
                                        </div>
                                    </div>
                                    <div class="Bubble-large-container go-down">
                                        <div class="multi-step-container h-100">
                                            <div class="Step-1 steps-transition h-100 w-100 z-4 position-absolute">
                                                <div class="video-container h-100">
                                                    <video class="video-content h-100"  loop id="Large-container-video" src="/files/users/<%=Bubble.UserId%>/Bubble-Videos/<%=Bubble.BubbleVideo%>"></video>
                                                    <div id="play-btn" class="video-controls"><i class="fa-solid fa-play fa-xl"></i></div>
                                                    <div id="pause-btn" class="video-controls"></div>
                                                    <div id="mute-btn" class="video-controls"><i class="fa-solid fa-volume-high"></i></div>
                                                    <div id="unmute-btn" class="video-controls"><i class="fa-solid fa-volume-xmark"></i></div>
                                                    <!--                                                <div id="stop-btn" class="video-controls">STOP</div>-->
                                                    <div id="replay-btn" class="video-controls"><i class="fa-solid fa-rotate-right fa-xl"></i></div>
                                                    <div id="Exit-btn" class="video-controls exit-btn"><i class="fa-solid fa-xmark fa-xl"></i></div>
                                                    <div class="video-progressbar">
                                                        <span class="h-100"></span>
                                                    </div>
                                                    <div id="step-1-btn" class="start-live-chat-btn multi-step-btn">
                                                        Start live chat
                                                    </div>
                                                    <div class="company-name">
                                                        <a href="#">   Created with <b> Complete Greet</b></a>
                                                    </div>
                                                    <div class="video-overlay">

                                                    </div>
                                                </div>
                                            </div>
                                            <div class="Step-2 steps-transition h-100 w-100 z-3 position-absolute">
                                                <div class="send-message-container h-100 w-100 position-relative">
<!--                                                    <div class="live-chat-name pt-3">LIVE CHAT</div>-->
<!--                                                    <div id="Exit-btn" class="video-controls exit-btn"><i class="fa-solid fa-xmark fa-xl"></i></div>-->
<!--                                                    <div id="Back-btn" class="video-controls back-btn"><i class="fa-solid fa-arrow-left fa-xl"></i></div>-->
<!--                                                    <div class="message-input ">-->
<!--                                                        <label class="Input-Style auto-input-focus w-75 mx-auto d-block mt-4 h-100">-->
<!--                                                            <textarea name="Start-Message" id="Start-Message" class="w-100 p-3" placeholder="Type here..."></textarea>-->
<!--                                                            <span class="multi-step-input-label">Let's start chat</span>-->
<!--                                                        </label>-->
<!--                                                    </div>-->
                                                        <div class="live-chat-name pt-3 mx-auto px-3 w-75">LIVE CHAT</div>
                                                        <div id="Exit-btn" class="video-controls exit-btn"><i class="fa-solid fa-xmark fa-xl"></i></div>
                                                        <div id="Back-btn" class="video-controls back-btn"><i class="fa-solid fa-arrow-left fa-xl"></i></div>
                                                        <div class="message-input ">
                                                            <label class="Input-Style auto-input-focus w-75 mx-auto d-block mt-4">
                                                                <input name="Name-Message" id="Name-Message" class="w-100 p-3" type="text" placeholder="Type here...">
                                                                <span  class="multi-step-input-label">Your Name</span>
                                                            </label>
                                                            <label class="Input-Style auto-input-focus w-75 mx-auto d-block mt-4">
                                                                <input name="Email-Message" id="Email-Message" class="w-100 p-3" type="email" placeholder="Type here...">
                                                                <span  class="multi-step-input-label">Your Email</span>
                                                            </label>
                                                            <div id="step-2-btn" style="bottom: auto" class="step-2-send-btn mt-2 mx-auto w-50 Button  multi-step-btn">
                                                                Send
                                                            </div>
                                                        </div>

<!--                                                        <div id="start-live-chat" class="mx-auto w-50 Button  multi-step-btn">-->
<!--                                                            Start live chat-->
<!--                                                        </div>-->

                                                </div>
                                            </div>
                                            <div class="Step-3 steps-transition h-100 w-100 z-2 position-absolute">

                                                <div class="send-message-container h-100 w-100 position-relative">
                                                    <div id="Exit-btn" class="video-controls exit-btn z-4"><i class="fa-solid fa-xmark fa-xl"></i></div>
                                                    <div id="Back-btn" class="video-controls back-btn z-4"><i class="fa-solid fa-arrow-left fa-xl"></i></div>
                                                    <%include ChatBox.ejs%>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-skeleton">
                                        <div class="animated-background">
                                            <div class="card-skeleton-img"></div>
                                            <div class="skel-mask-container">
                                                <div class="skel-mask skel-mask-1"></div>
                                                <div class="skel-mask skel-mask-2"></div>
                                                <div class="skel-mask skel-mask-3"></div>
                                                <div class="skel-mask skel-mask-4"></div>
                                                <div class="skel-mask skel-mask-5"></div>
                                                <div class="skel-mask skel-mask-6"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tabs-size d-flex flex-row mt-3 justify-content-center">
